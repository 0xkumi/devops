pipeline {
    agent {label 'docker && ansible'}
    stages {	
        stage('Build incognito') {
            steps {
                build(job: "Build_IncognitoTestnet", wait: true )
            }
        }
        stage('Deploy incognito') {
            steps {
                build(job: "Deploy_IncognitoTestnet", wait: true )
            }
        }
    }
}